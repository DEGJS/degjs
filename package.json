{
	"name": "degjs-website",
	"version": "1.0.1",
	"description": "The DEGJS website",
	"scripts": {
		"build-css": "postcss -c postcss-config.json",
		"build-html": "node lib/indexPageBuilder",
		"build": "npm run build-css && npm run build-html",
		"update-repos": "node lib/reposUpdater",
		"update-demo": "npm run-script update-repos && npm run build-css && npm run build-html"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DEGJS/degjs.git"
	},
	"keywords": [
		"DEGJS"
	],
	"author": "Ryan Heap",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/DEGJS/degjs/issues"
	},
	"homepage": "https://degjs.com",
	"devDependencies": {
		"autoprefixer": "^6.4.0",
		"postcss": "^5.1.2",
		"postcss-cli": "^2.5.2",
		"postcss-custom-properties": "^5.0.1",
		"postcss-import": "^8.1.2",
		"postcss-nested": "^1.0.0",
		"request": "^2.74.0"
	}
}
